    "history": "^5.3.0",
    "http-status-codes": "^2.1.4",
    "i18next": "^21.9.1",
    "i18next-browser-languagedetector": "^6.1.5",
    "i18next-http-backend": "^1.4.1",
    "moment": "^2.29.4",
    "msw": "^0.47.2",
    "react": "^18.2.0",
    "react-bootstrap": "^2.5.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "7.35.0",
    "react-i18next": "^11.18.6",
    "react-modal-hook": "^3.0.2",
    "react-phone-input-2": "^2.15.1",
    "react-redux": "^8.0.2",
