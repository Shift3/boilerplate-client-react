{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.21.1",
    "bootstrap": "^4.6.0",
    "dotenv": "^8.2.0",
    "husky": "^5.1.3",
    "react": "^17.0.1",
    "react-bootstrap": "^1.5.2",
    "react-dom": "^17.0.1",
    "react-hook-form": "^7.3.6",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "sentry": "^0.1.2",
    "source-map-explorer": "^2.5.2",
    "styled-components": "^5.2.1",
    "web-vitals": "^1.0.1",
    "yup": "^0.32.9"
  },
  "scripts": {
    "start": "cross-env PORT=4200 react-scripts start",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "build": "react-scripts build",
    "build:staging": "env-cmd -f ./.env.staging react-scripts build",
    "deploy:staging": "yarn build:staging && aws s3 sync $BUILD_DIRECTORY_PATH s3://$AWS_SANDBOX_URL --profile shift3 --delete",
    "test": "react-scripts test",
    "test-ci": "react-scripts test --watchAll=false",
    "test-cov": "react-scripts test --coverage",
    "test-cov-ci": "react-scripts test --watchAll=false --coverage",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@hookform/resolvers": "^2.4.0",
    "@sentry/react": "^6.2.4",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-bootstrap": "^0.32.25",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-test-renderer": "^17.0.1",
    "@types/styled-components": "^5.1.8",
    "@types/yup": "^0.29.11",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "cross-env": "^7.0.3",
    "env-cmd": "^10.1.0",
    "eslint": "^7.26.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest-dom": "^3.8.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^4.2.0",
    "jest-styled-components": "^7.0.4",
    "moment": "^2.29.1",
    "prettier": "^2.2.1",
    "react-test-renderer": "^17.0.2",
    "react-testing-library": "^8.0.1",
    "typescript": "^4.1.2",
    "typescript-plugin-styled-components": "^1.5.0"
  }
}
